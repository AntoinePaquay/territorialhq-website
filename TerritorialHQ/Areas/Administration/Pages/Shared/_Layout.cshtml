<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - TerritorialHQ Administration</title>
    <link rel="stylesheet" href="~/css/reboot.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/elektra.css" asp-append-version="true" />
    @await RenderSectionAsync("Styles", required: false)

</head>
<body class="bg-slate-950">

    <partial name="_TopBar" />

    <main>
        <div class="admin pa-40">
            @RenderBody()
        </div>
    </main>


    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/discord-loader.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>